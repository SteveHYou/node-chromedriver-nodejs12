{
  "name": "chromedriver-nodejs12",
  "version": "116.0.0",
  "keywords": [
    "chromedriver",
    "selenium"
  ],
  "description": "ChromeDriver for Selenium, modified to run on nodejs 12. Full credits go to Giovanni Bassi for original node-chromedriver project (https://github.com/giggio/node-chromedriver)",
  "homepage": "https://github.com/SteveHYou/node-chromedriver-nodejs12",
  "repository": {
